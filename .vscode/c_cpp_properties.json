{
  "env": {
    "devkitpro": "/opt/devkitpro",
    "devkitarm": "C:/devkitPro/devkitARM",
    "ctrulib": "C:/devkitPro/libctru",
    "3ds-curl": "C:/devkitPro/portlibs/3ds/"
  },
  "configurations": [
    {
      "name": "windows-gcc-x64",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "defines": [
        "ARM11",
        "_3DS"
      ],
      "compilerPath": "C:/TDM-GCC-64/bin/gcc.exe",
      "browse": {
        "path": [
          "${workspaceFolder}/libctrpf/include/**",
          "${ctrulib}/include/**",
          "${3ds-curl}/include/**"
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      },
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "windows-gcc-x64"
    }
  ],
  "version": 4
}